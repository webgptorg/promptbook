        "generate-packages": "ts-node ./scripts/generate-packages/generate-packages.ts --commit",
        "generate-documentation": "ts-node ./scripts/generate-packages/generate-packages.ts --commit",
        "import-markdowns": "deno --allow-env --allow-read --allow-write --allow-sys --unstable-sloppy-imports ./book/scripts/import-markdown/import-markdown.ts && cd ./book && git add . && git commit --message \"üìÑ Import markdowns\" && cd .. && git add . && git commit --message \"üìÑ Import markdowns\"",
