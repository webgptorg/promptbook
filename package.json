    "scripts": {
        "test": "jest",
        "test-types": "tsc",
        "lint": "eslint src",
        "preversion": "git pull && npm run find-name-discrepancies && npm run make-promptbook-collection && npm run repair-imports && npm run lint && npm test && npm run test-types && npm run test-package-generation",
        "postversion": "npm run update-version-in-config && npm run generate-examples-jsons && npm run generate-packages && npm run generate-documentation && npm run import-markdowns && npm run generate-openapi-types && git push && git push --tags && npm run use-packages",
        "generate-packages": "ts-node ./scripts/generate-packages/generate-packages.ts --commit",
        "generate-documentation": "ts-node ./scripts/generate-documentation/generate-documentation.ts --commit",
        "generate-examples-jsons": "ts-node ./scripts/generate-examples-jsons/generate-examples-jsons.ts --commit",
        "generate-openapi-types": "npx openapi-typescript src/remote-server/openapi.yml -o src/remote-server/openapi-types.ts --immutable --export-type && git add . && git commit --allow-empty --message \"üè≠ Generate OpenAPI types\" && git push",
        "import-markdowns": "npx deno --allow-env --allow-read --allow-write --allow-sys --unstable-sloppy-imports ./book/scripts/import-markdown/import-markdown.ts && cd ./book && git add . && git commit --allow-empty --message \"üìÑ Import markdowns\" && git push && cd .. && git add . && git commit --allow-empty --message \"üìÑ Import markdowns\"",
        "find-name-discrepancies": "ts-node ./scripts/find-name-discrepancies/find-name-discrepancies.ts",
        "test-package-generation": "npx ts-node ./scripts/generate-packages/generate-packages.ts --skip-bundler  --commit",
        "make-promptbook-collection": "ts-node ./scripts/make-promptbook-collection/make-promptbook-collection.ts --commit",
        "repair-imports": "ts-node ./scripts/repair-imports/repair-imports.ts --commit",
        "update-version-in-config": "ts-node ./scripts/update-version-in-config/update-version-in-config.ts --commit",
        "use-packages": "ts-node ./scripts/use-packages/use-packages.ts"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
